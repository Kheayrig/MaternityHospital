<Window x:Class="MaternityHospital.View.Windows.ReportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:MaternityHospital.View.UserControls"
        mc:Ignorable="d"
        Title="Заключение" Height="450" Width="800" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
         Background="#DDCDFF"
        >
    <StackPanel>
        <Label FontSize="35" HorizontalAlignment="Center" VerticalAlignment="Center">Дополните заключение</Label>
        <TextBox FontSize="15" Height="300" Width="700" TextWrapping="Wrap" AcceptsReturn="True" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBox>
        <Button Height="40" Width="200" Margin="0,10,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" Click="Button_Click">Сохранить</Button>
    </StackPanel>
</Window>
